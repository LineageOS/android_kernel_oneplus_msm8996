// msm-pmi8994.dtsi
&spmi_bus {
	qcom,pmi8994@2 {
		qcom,leds@a300 {
			compatible = "qcom,leds-qpnp";
			status = "okay";
			reg=<0xa300 0x100>;
			qcom,led_mpp_4 {
				label = "mpp";
				linux,name = "button-backlight";
				linux,default-trigger = "none";
				qcom,default-state = "off";
				qcom,max-current = <40>;
				qcom,current-setting = <5>;
				qcom,id = <6>;
				qcom,mode = "manual";
				qcom,source-sel = <1>;
				qcom,mode-ctrl = <0x60>;
			};
		};
	};

	qcom,pmi8994@3 {
		qcom,leds@d000 {
			qcom,rgb-sync;
		};
	};
};

